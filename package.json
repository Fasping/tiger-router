{
  "name": "tiger-router",
  "version": "1.0.2",
  "description": "A minimalistic routing for React. Nothing else",
   "keywords": [
    "react",
    "minimalistic",
    "router",
    "tiny",
    "routing",
    "hooks"
  ],
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "swc ./src/*.jsx -d lib && rm lib/App.js lib/main.js lib/Router.test.js && cp src/*.js lib",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "author": "Fernando Cases <nandocasesgarcia@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "path-to-regexp": "6.2.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.36",
    "@testing-library/dom": "9.0.0",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react-swc": "3.0.0",
    "@vitest/ui": "0.28.5",
    "happy-dom": "8.7.1",
    "standard": "17.0.0",
    "vite": "4.1.0",
    "vitest": "0.28.5"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ]
  }
}
